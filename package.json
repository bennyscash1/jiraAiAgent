{
  "name": "bugdraft-monorepo",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "server",
    "web"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm --filter server dev\" \"pnpm --filter web dev\"",
    "build": "pnpm -r build",
    "start": "pnpm --filter server start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

